{
	"name": "Wikipedia-inator",
	"version": "0.0.1",
	"description": "Adds '?useskin=vector' to the end of Wikipedia links in order to put them in the old vector format.",
	"permissions": ["declarativeNetRequest"],
	"declarative_net_request": {
		"rule_resources": [{
			"id": "ruleset_1",
			"enabled": true,
			"path": "rules.json"
		}]
	},
	"host_permissions": ["*://*.wikipedia.org/*"],
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},
	"manifest_version": 3
}